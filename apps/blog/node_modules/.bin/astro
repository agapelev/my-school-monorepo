#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/lev/my-school-monorepo/node_modules/.pnpm/astro@4.16.19_@types+node@24.9.2_lightningcss@1.30.2_rollup@4.52.5_typescript@5.9.3/node_modules/astro/node_modules:/home/lev/my-school-monorepo/node_modules/.pnpm/astro@4.16.19_@types+node@24.9.2_lightningcss@1.30.2_rollup@4.52.5_typescript@5.9.3/node_modules:/home/lev/my-school-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/lev/my-school-monorepo/node_modules/.pnpm/astro@4.16.19_@types+node@24.9.2_lightningcss@1.30.2_rollup@4.52.5_typescript@5.9.3/node_modules/astro/node_modules:/home/lev/my-school-monorepo/node_modules/.pnpm/astro@4.16.19_@types+node@24.9.2_lightningcss@1.30.2_rollup@4.52.5_typescript@5.9.3/node_modules:/home/lev/my-school-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
